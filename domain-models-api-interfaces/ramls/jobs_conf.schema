{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Configuration for Jobs",
  "description": "Includes type and category of job, and whether it is enabled.",
  "type": "object",
  "properties": {
    "_id": {
      "type": "string"
    },
    "module": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "category": {
      "type": "string"
    },
    "enabled": {
      "type": "boolean"
    },
    "creator": {
      "type": "string"
    },
    "inst_id": {
      "type": "string"
    },
    "lib_id": {
      "type": "string"
    },
    "bulk_size": {
      "type": "integer"
    },
    "fail_percentage": {
      "type": "number"
    },
    "last_modified": {
      "type": "object",
      "$ref": "last_modified.schema"
    },
    "parameters": {
      "type": "object",
      "$ref" : "raml-util/schemas/parameters.schema"
    }
  },
  "required": [
    "module",
    "name",
    "type",
    "enabled",
    "inst_id"
  ]
}
